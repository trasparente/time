{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "time",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "date": {
        "type": "string",
        "format": "date",
        "default": "today",
        "autocomplete": "off"
      },
      "category": {
        "type": "string",
        "autocomplete": "on"
      },
      "details": {
        "type": "string",
        "autocomplete": "on"
      },
      "value": {
        "type": "number"
      },
      "duration": {
        "type": "string",
        "format": "duration",
        "autocomplete": "off"
      }
    }
  }
}